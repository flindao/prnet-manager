   sleep 5s
   ;;
   8 | 08)
   clear
   infousers
   echo -ne "\n\033[1;33mENTER \033[1;33mPARA VOLTAR AO \033[1;33mMENU!\033[0m"; read
   ;;
   9 | 09)
   clear
   sshmonitor.x
   echo -ne "\n\033[1;33mENTER \033[1;33mPARA VOLTAR AO \033[1;33mMENU!\033[0m"; read
   ;;
   10 | 10)
   clear
   banner
   sleep 5s
   ;;
   11 | 11)
   clear
   userbackup
   ;;
   12 | 12)
   clear
