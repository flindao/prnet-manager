   clear
   criarteste
   echo -ne "\n\033[1;33mENTER \033[1;33mPARA VOLTAR AO \033[1;33mMENU!\033[0m"; read
   ;;
   3 | 03)
   clear
   alterarsenha
   sleep 5s
   ;;
   4 | 04)
   clear
   mudardata
   sleep 5s
   ;;
   5 | 05)
   clear
   alterarlimite
   sleep 5s
   ;;
   6 | 06)
   clear
   remover
   sleep 5s
   ;;
   7 | 07)
   clear
   expcleaner;uexpired
